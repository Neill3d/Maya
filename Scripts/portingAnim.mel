//--------------------------------------------------------------------// Copy all animation keys from one list of effectors to another list//// Hips//select -r HipsEff ;copyKey -time ":" -hierarchy none -controlPoints 0 -shape 1 {"Run__hips"};select -r Player_Character_Ctrl:HipsEffector ;pasteKey -time 0 -float 0 -option insert -copies 1 -connect 1 -timeOffset 0 -floatOffset 0 -valueOffset 0 {"_hips"};//// spine & neck//select -r SpineEff ;copyKey -time ":" -hierarchy none -controlPoints 0 -shape 1 {"Run__spine"};select -r Player_Character_Ctrl:ChestOriginEffector ;pasteKey -time 0 -float 0 -option insert -copies 1 -connect 1 -timeOffset 0 -floatOffset 0 -valueOffset 0 {"_spine"};//// head//select -r HeadEff ;copyKey -time ":" -hierarchy none -controlPoints 0 -shape 1 {"Run__head"};select -r Player_Character_Ctrl:HeadEffector ;pasteKey -time 0 -float 0 -option insert -copies 1 -connect 1 -timeOffset 0 -floatOffset 0 -valueOffset 0 {"_head"};//// left leg//select -r LeftFootEff ;copyKey -time ":" -hierarchy none -controlPoints 0 -shape 1 {"Run__leftLeg"};select -r Player_Character_Ctrl:LeftAnkleEffector ;pasteKey -time 0 -float 0 -option insert -copies 1 -connect 1 -timeOffset 0 -floatOffset 0 -valueOffset 0 {"_leftLeg"};//// right leg//select -r RightFootEff ;copyKey -time ":" -hierarchy none -controlPoints 0 -shape 1 {"Run__rightLeg"};select -r Player_Character_Ctrl:RightAnkleEffector ;pasteKey -time 0 -float 0 -option insert -copies 1 -connect 1 -timeOffset 0 -floatOffset 0 -valueOffset 0 {"_rightLeg"};//// left hand//select -r LeftHandEff ;copyKey -time ":" -hierarchy none -controlPoints 0 -shape 1 {"Run__leftArm"};select -r Player_Character_Ctrl:LeftWristEffector ;pasteKey -time 0 -float 0 -option insert -copies 1 -connect 1 -timeOffset 0 -floatOffset 0 -valueOffset 0 {"_leftArm"};//// right hand//select -r RightHandEff ;copyKey -time ":" -hierarchy none -controlPoints 0 -shape 1 {"Run__rightArm"};select -r Player_Character_Ctrl:RightWristEffector ;pasteKey -time 0 -float 0 -option insert -copies 1 -connect 1 -timeOffset 0 -floatOffset 0 -valueOffset 0 {"_rightArm"};